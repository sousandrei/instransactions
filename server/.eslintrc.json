{
	"env": {
		"es6": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 8
	},
	"rules": {
		"indent": [
			2,
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"quotes": [
			2,
			"single"
		],
		"semi": [
			2,
			"never"
		],
		"no-undef": 1,
		"no-unused-vars": 1,
		"no-console": 0,
		"max-len": [
			2,
			80
		]
	}
}